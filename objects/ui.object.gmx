        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(!show_ui)
    exit; 

var txt = "layer-core#";
txt += "Presented by ~elementbound#";

if(show_fps == 1) {
    txt += concat("~", fps, "#");
} else if(show_fps == 2) {
    var attribute; 
    if(fps &lt; 20)
        attribute = "stuttering";
    else if(fps &lt; 30)
        attribute = "chunky";
    else if(fps &lt; 60)
        attribute = "smooth";
    else if(fps &lt; 120)
        attribute = "glamorous";
    else if(fps &lt; 144)
        attribute = "glorious"; 
    else 
        attribute = "perplexingly silky";
        
    txt += concat("#Currently the demo is sailing at a ", attribute, " ", fps, " fps#");
}

if(show_help) {
    txt += "#Help: #";
    txt += "[Q] Lose textures#";
    txt += "[W] Toggle help#";
    txt += "[E] Toggle FPS#";
    txt += "[R] Toggle UI (this whole thing)#";
    txt += "[T] Toggle asset manager log ( bottom left log )#";
    txt += "[Esc] Escape";
}

draw_text(0,0, txt); 
</string>
