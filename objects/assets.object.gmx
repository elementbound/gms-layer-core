            <string>/// Load queued resources 
if(ds_queue_empty(queue)) 
    working = false; 
else {
    working = true;
    
    var type = ds_queue_dequeue(queue);
    var name = ds_queue_dequeue(queue); 
    var data = ds_queue_dequeue(queue); 
    
    if(type == "texture")
        type = default_textype;
        
    if(type == "texture/volatile") 
        textures[?name] = texture_load_volatile(data);
    else if(type == "texture/permanent")
        textures[?name] = texture_load_permanent(data);
    else if(type = "mesh/gen") {
        var detail = ds_queue_dequeue(queue); 
        meshes[?name] = script_execute(data, detail); 
    }
    
    log += concat('[', type, ']', name, '#');
    log_reset = max(get_time()+1000, log_reset+1000);
}
